{
  "manifest_version": 2,
  "name": "Re-ibu",
  "version": "8.0",
  "description": "Redirect to custom privacy-friendly alternatives",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Re-ibu",
    "default_popup": "popup.html",
    "default_icon": {
      "128": "./logo.png"
    }
  }
}